<?php
$user = "arduino";
$pass = "Bi0T2";
$host = "localhost";	
$db = "biot";

//$dbh = mysqli_connect($MyHostname , $MyUsername, $MyPassword, "biot");


$mysqli = new mysqli($host,$user,$pass,$db);
if ($mysqli->connect_errno) {
    echo "Echec lors de la connexion à MySQL: (" . $mysqli->connect_errno . ") " . $mysqli->connect_error. "<br />";
}


if (!$mysqli->multi_query("CALL GetSensorsValue(3)")) {
    echo "Echec lors de l'appel à CALL : (" . $mysqli->errno . ") " . $mysqli->error. "<br />";
}



do {
    if ($res = $mysqli->store_result()) {
        printf("---\n");
        print_r($res->fetch_all());
        $res->free();
    } else {
        if ($mysqli->errno) {
            echo "Echec de STORE : (" . $mysqli->errno . ") " . $mysqli->error;
        }
    }
} while ($mysqli->more_results() && $mysqli->next_result());


?>
